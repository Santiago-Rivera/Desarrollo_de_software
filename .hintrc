{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "disown-opener": "off"
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  }
}